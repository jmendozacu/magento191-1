    <default>
        <netstarter_retaildirections>
            <product>
                <tax_id>2</tax_id>
            </product>
        </netstarter_retaildirections>
        <giftcardsapi>
            <netstarter_retaildirections>
                <active>1</active>
                <model>netstarter_retaildirections/giftcard</model>
                <name>Retail Directions</name>
                <title>Retail Directions</title>
            </netstarter_retaildirections>
        </giftcardsapi>
    </default>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <Netstarter_Retaildirections after="Mage_Adminhtml">Netstarter_Retaildirections_Adminhtml</Netstarter_Retaildirections>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <adminhtml>
        <layout>
            <updates>
                <retaildirections>
                    <file>retaildirections.xml</file>
                </retaildirections>
            </updates>
        </layout>
    </adminhtml>
    <default>
        <netstarter_retaildirections>
            <order>
                <giftwrapunitprice>5.99</giftwrapunitprice>
            </order>
        </netstarter_retaildirections>
    </default>
