<config>
    <sections>
        <catalog translate="label" module="catalog">
            <groups>
                <frontend translate="label">
                    <fields>
                        <active_root_category translate="label comment">
                            <label>Active Root Category Id</label>
                            <comment>Used to populate child category links throughout the site</comment>
                            <frontend_type>text</frontend_type>
                            <!--<source_model>adminhtml/system_config_source_catalog_gridPerPage</source_model>-->
                            <sort_order>250</sort_order>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </active_root_category>
                    </fields>
                </frontend>
            </groups>
        </catalog>
    </sections>

    <sections>
        <contacts translate="label" module="contacts">
            <groups>
                <form translate="label">
                    <label>Contact Form</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>10</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <inquiry_types translate="label">
                            <label>Types of Inquiry</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </inquiry_types>
                    </fields>
                </form>
            </groups>
        </contacts>
    </sections>
</config>